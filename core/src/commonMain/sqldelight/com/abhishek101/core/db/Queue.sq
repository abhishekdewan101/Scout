CREATE TABLE Queue (
slug TEXT NOT NULL,
name TEXT NOT NULL,
coverUrl TEXT NOT NULL,
position INTEGER NOT NULL
);

isGameQueued:
SELECT COUNT(*) FROM Queue WHERE slug=?;

selectAllGames:
SELECT * FROM Queue;

getCurrentNumberOfGamesInQueue:
SELECT MAX(position) FROM Queue;

insertGameIntoQueue:
INSERT INTO Queue
VALUES(?,?,?,?);

updatePositionOfGame:
UPDATE Queue
SET position = ?
WHERE slug = ?;